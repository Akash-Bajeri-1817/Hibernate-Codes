<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<!-- To provide mapping between entity class and student -->
	<class name="com.tca.entities.Employee"
		table="Employee">

		<!-- Mapping between fields and column name -->
		<id name="employeeId" column="EMPLOYEE_ID" />
		<property name="name" column="EMPLOYEE_NAME" />
		<property name="salary" column="SALARY" />
		<property name="city" column="CITY" />
		<property name="designation" column="DESIGNATION"/>
	</class>
</hibernate-mapping>