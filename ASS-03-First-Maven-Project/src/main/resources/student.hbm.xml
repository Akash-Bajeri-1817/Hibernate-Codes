<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

    
 <hibernate-mapping>
 	<!-- To provide mapping between entity class and student-->
 	<class name="com.tca.entities.Student" table="student">
 		
 		<!-- Mapping between fields and column name-->
 		<id name="rollNumber" column="RNO"/>
 		<property name="name" column="NAME"/>
 		<property name="percentage" column="PER"/>
 	</class>
 </hibernate-mapping>